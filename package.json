{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package:main": "ncc build --source-map --license licenses.txt",
    "package:cleanup": "ncc build ./src/cleanup.ts -o cleanup --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package:main && npm run package:cleanup && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "nanoid": "^3.3.6"
  },
  "devDependencies": {
    "@alergeek-ventures/eslint-config": "^7.0.8",
    "@types/localtunnel": "^2.0.1",
    "@types/node": "^20.4.8",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.45.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  }
}
